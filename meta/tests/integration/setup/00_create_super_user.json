{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://localhost/?slatAction=main.login"
    },
    {  
         "height":902.4,
         "type":"setWindowSize",
         "width":1440
    },
    {  
         "type":"include",
         "rel_dir":"../se-interpreter/components/account/account_settings_co.json"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "firstName"
      },
      "text": "${account_login_name}"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "lastName"
      },
      "text": "${account_login_last_name}"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "company"
      },
      "text": "${account_company}"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "emailAddress"
      },
      "text": "${email_account}"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "emailAddressConfirm"
      },
      "text": "${email_account}"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "password"
      },
      "text": "${account_password}"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "passwordConfirm"
      },
      "text": "${account_password}"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//form[@id='adminCreateSuperUserForm']//button[.='Create & Login']"
      }
    },
    {
      "script":"return document.readyState",
      "type":"waitForEval",
      "value":"complete"
    },
    {
      "type": "verifyTextPresent",
      "text": "Dashboard"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}
