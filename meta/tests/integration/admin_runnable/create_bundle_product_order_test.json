      },
      {   
         "type":"include",
         "rel_dir":"../se-interpreter/components/account/account_settings_co.json"
      },
      {  
         "type":"include",
         "rel_dir":"/components/location/create_location_test.json"
      },
      {  
         "type":"include",
         "rel_dir":"/components/brand/create_brand_test.json"
      },
      {  
         "type":"include",
         "rel_dir":"/components/account/create_guest_account_test.json"
      },
      {  
         "type":"include",
         "rel_dir":"/components/products/bundle/create_bundle_product_test.json"
      }, 
      {  
         "type":"include",
         "rel_dir":"/components/order/order_settings_co.json"
      },
      {  
         "type":"include",
         "rel_dir":"/components/order/bundle/bundle_product_order_settings_co.json"
