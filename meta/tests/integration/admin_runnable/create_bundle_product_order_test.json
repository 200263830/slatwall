      {  
         "type":"clickElement",
         "locator":{  
            "type":"link text",
            "value":"Orders"
         }
      },
      {  
         "type":"clickElement",
         "locator":{  
            "type":"xpath",
            "value":"//div[@id='j-mobile-nav']/ul/li[3]/ul/li[1]/a"
         }
      },
      {  
         "type":"include",
         "rel_dir":"/components/order/create_order_with_guest_account_co.json"
      },
      {  
         "type":"include",
         "rel_dir":"/components/order/assert_order_created_co.json"
      },
      {  
         "__comment":"Close the accordion as it has caused trouble with firefox before",
         "type":"clickElement",
         "locator":{  
            "type":"css selector",
            "value":" #accordion > div:nth-child(1) > a"
         }
      },
      {  
         "type":"setElementText",
         "locator":{  
            "type":"name",
            "value":"quantity"
         },
         "text":"${quantity}"
      },
      {  
         "type":"include",
         "rel_dir":"/components/order/bundle/add_sku_with_email_fullfilment_co.json"
      },
      {  
         "type":"include",
         "rel_dir":"/components/order/bundle/assert_added_sku_with_email_fullfilment_co.json"
      },
      {  
         "type":"include",
         "rel_dir":"/components/order/place_order_credit_card_co.json"
      },
      {  
         "__comment":"Back to the order list",
         "type":"clickElement",
         "locator":{  
            "type":"css selector",
            "value":" #j-main-content > div > div.row.s-body-nav > nav > div:nth-child(2) > div > div:nth-child(1) > a"
         }
      },
      {  
         "type":"include",
         "rel_dir":"/components/order/assert_order_created_in_list_order_co.json"
      },
      {  
         "type":"include",
         "rel_dir":"/components/logout_co.json"
      }
   ],
   "data":{  
      "configs":{  

      },
      "source":"none"
   },
   "inputs":[  

   ],
   "timeoutSeconds":60
}