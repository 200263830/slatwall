{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://localhost/"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "emailAddress"
      },
      "text": "qa@mailinator.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "password"
      },
      "text": "QaPa33word"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//form[@id='j-login-wrapper']//button[.='Login']"
      }
    },
    {
      "type": "waitForElementPresent",
      "locator": {
        "type": "link text",
        "value": "Config"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Config"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Locations"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Add Location"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "locationName"
      },
      "text": "lucknow"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "button.btn.btn-success"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='accordion']//span[.='Location Configurations']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "a.adminentitycreatelocationconfiguration.btn"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "locationConfigurationName"
      },
      "text": "helloworld"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "locationConfigurationCapacity"
      },
      "text": "21"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='btn-toolbar']//button[.=' Save']"
      }
    },
    {
      "type": "waitForElementPresent",
      "locator": {
        "type": "link text",
        "value": "Products"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Products"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='j-mobile-nav']//a[.='Products']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='btn-toolbar']//button[.=' Create ']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Event Product"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "product.productName"
      },
      "text": "BihariShaddi"
    },
    {
      "type": "storeEval",
      "script": "return 'sunny'+Math.floor(Math.random()*1000000)",
      "variable": "uniqueCode"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "product.productCode"
      },
      "text": "${uniqueCode}"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "price"
      },
      "text": "100"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "name",
        "value": "eventStartDateTime"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "24"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "name",
        "value": "eventEndDateTime"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "29"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "i.hibachi-ui-checkbox"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "button.btn.btn-primary"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "${uniqueCode}"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}