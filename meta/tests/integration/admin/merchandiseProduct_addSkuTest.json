{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://localhost/"
    },
    {
      "type": "setWindowSize",
      "width": "1440",
      "height": "902.5"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "emailAddress"
      },
      "text": "qa@mailinator.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "password"
      },
      "text": "QaPa33word"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//form[@id='j-login-wrapper']//button[.='Login']"
      }
    },
    {
      "type": "waitForEval",
      "script": "return document.readyState",
      "value": "complete"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Products"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='j-mobile-nav']//a[.='Products']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='btn-toolbar']//button[.=' Create ']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Merchandise Product"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "product.productName"
      },
      "text": "testEntity"
    },
    {
      "type": "storeEval",
      "script": "return 'sunny'+Math.floor(Math.random()*10000)",
      "variable": "code"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "product.productCode"
      },
      "text": "${code}"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "price"
      },
      "text": "40"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "button.btn.btn-primary"
      }
    },
    {
      "type": "waitForEval",
      "script": "return document.readyState",
      "value": "complete"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='accordion']//span[.='Skus']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='tabskus']//a[.='Add Sku']"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "newSku.skuName"
      },
      "text": "kit"
    },
    {
      "type": "storeEval",
      "script": "return Math.floor(Math.random()*100000)",
      "variable": "skuCode"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "newSku.skuCode"
      },
      "text": "${skuCode}"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "newSku.price"
      },
      "text": "50.00"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "newSku.listPrice"
      },
      "text": "10.00"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//form[@id='adminentityprocessproduct_addsku']//button[.=' Add Sku']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='accordion']//span[.='Skus']"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "${skuCode}"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}