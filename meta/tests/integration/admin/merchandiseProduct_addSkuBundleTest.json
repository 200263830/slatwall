{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://localhost/"
    },
    {
      "type": "setWindowSize",
      "width": "1990",
      "height": "902.5"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "emailAddress"
      },
      "text": "qa@mailinator.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "password"
      },
      "text": "QaPa33word"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//form[@id='j-login-wrapper']//button[.='Login']"
      }
    },
    {
      "type": "waitForEval",
      "script": "return document.readyState",
      "value": "complete"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Products"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='j-mobile-nav']//a[.='Products']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='btn-toolbar']//button[.=' Create ']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Merchandise Product"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "product.productName"
      },
      "text": "barfi"
    },
    {
      "type": "storeEval",
      "script": "return 'sunny'+ Math.floor(Math.random()*1000)",
      "variable": "proCode"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "product.productCode"
      },
      "text": "${proCode}"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "price"
      },
      "text": "90"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "button.btn.btn-primary"
      }
    },
    {
      "type": "waitForEval",
      "script": "return document.readyState",
      "value": "complete"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='accordion']/div[2]/a/div/h4"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Add Sku Bundle"
      }
    },
    {
      "type": "storeEval",
      "script": "return Math.floor(Math.random()*1000)",
      "variable": "code"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "skuCode"
      },
      "text": "${code}"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "price"
      },
      "text": "50"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "i.hibachi-ui-checkbox"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "button.btn.btn-primary"
      }
    },
    {
      "type": "waitForEval",
      "step_name": "25.",
      "script": "return document.readyState",
      "value": "complete"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='accordion']//span[.='Skus']"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "${code}"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}