    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "password"
      },
      "text": "QaPa33word"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//form[@id='j-login-wrapper']//button[.='Login']"
      }
    },
    {
      "type": "waitForElementPresent",
      "locator": {
        "type": "link text",
        "value": "Products"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Products"
      }
    },
    {
