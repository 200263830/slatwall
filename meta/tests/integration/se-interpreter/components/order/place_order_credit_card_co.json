{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [ 
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='btn-toolbar']//button[.=' Actions ']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Place Order"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "newOrderPayment.creditCardNumber"
      },
      "text": "${credit_card_number}"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "newOrderPayment.nameOnCreditCard"
      },
      "text": "${account_login_name} ${account_login_last_name}"
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "css selector",
        "value": "div.s-property.form-horizontal > div.hibachi-display-toggle > div.hibachi-display-toggle > div:nth-child(5) > div > select > option:nth-child(4)"
      }
    },
  
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "newOrderPayment.securityCode"
      },
      "text": "${security_code}"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "newOrderPayment.billingAddress.name"
      },
      "text": "${account_login_name} ${account_login_last_name}"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "newOrderPayment.billingAddress.company"
      },
      "text": "${account_company}"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "newOrderPayment.billingAddress.streetAddress"
      },
      "text": "${add_account_address_street_address}"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "newOrderPayment.billingAddress.city"
      },
      "text": "${add_account_address_city}"
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='slatwall-address-container']/div[7]/div/select//option[26]"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "newOrderPayment.billingAddress.postalCode"
      },
      "text": "${add_account_address_postal_code}"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//form[@id='adminentityprocessorder_placeorder']//button[normalize-space(.)='Place Order']"
      }
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}