{  
   "steps":[  
      {  
         "type":"include_as_subcomponent",
         "rel_dir":"components/location/open_location_address_accordion_co.json"
      },
      {  
         "type":"include_as_subcomponent",
         "rel_dir":"components/config/settings_co.json"
      },
      {  
         "type":"setElementText",
         "locator":{  
            "type":"name",
            "value":"address.name"
         },
         "text":"${text}"
      },
      {  
         "type":"setElementText",
         "locator":{  
            "type":"name",
            "value":"address.streetAddress"
         },
         "text":"${street_address}"
      },
      {  
         "type":"setElementText",
         "locator":{  
            "type":"name",
            "value":"address.city"
         },
         "text":"${city}"
      },
      {  
         "type":"setElementSelected",
         "locator":{  
            "type":"xpath",
            "value":"//div[@class='slatwall-address-container']/div[6]/div/select//option[26]"
         }
      },
      {  
         "type":"setElementText",
         "locator":{  
            "type":"name",
            "value":"address.postalCode"
         },
         "text":"${postalCode}"
      },
      {  
         "type":"clickElement",
         "locator":{  
            "type":"css selector",
            "value":"button.btn.btn-success"
         }
      }
   ]
}